{
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "outputDirectory": "dist/public",
  "functions": {
    "api/index.js": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/trpc/:path*",
      "destination": "/api"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
